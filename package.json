{
  "name": "sneaky-sasquatch-mcp",
  "version": "1.0.0",
  "description": "MCP server for the Sneaky Sasquatch game wiki - provides searchable access to ~986 wiki articles",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "scrape": "node dist/scripts/scrape.js",
    "dev": "tsc --watch"
  },
  "keywords": ["mcp", "sneaky-sasquatch", "wiki", "fandom"],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "better-sqlite3": "^11.8.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.13.4",
    "typescript": "^5.7.3"
  }
}
